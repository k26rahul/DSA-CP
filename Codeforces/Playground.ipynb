   "source": [
    "cache = dict()\n",
    "\n",
    "\n",
    "def solve(str):\n",
    "  if len(str) == 2:\n",
    "    return 2\n",
    "\n",
    "  if str in cache:\n",
    "    return cache[str]\n",
    "\n",
    "  ans = solve(str[1:]) * solve(str[:-1])\n",
    "  cache[str] = ans\n",
    "  return ans\n",
    "\n",
    "\n",
    "solve('ababa')"
   ]
  }
